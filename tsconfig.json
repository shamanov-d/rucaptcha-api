{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./lib",
		"typeRoots": [
			"node_modules/@types"
		],
		"strict": true,
		"declaration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true //запрет на регистр ссылок 
	},
	"include": [
		"src"
	]
}